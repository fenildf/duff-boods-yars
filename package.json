{
  "name": "yars",
  "version": "0.0.1",
  "description": "Yet Another React Starter",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint ./client/**/*.css",
    "clean": "rm -rf dist",
    "dev": "webpack-dev-server --config webpack.config.development.babel.js --host 0.0.0.0",
    "dev:dashboard": "webpack-dashboard -- webpack-dev-server --config webpack.config.development.babel.js",
    "prod": "NODE_ENV=production webpack --config webpack.config.production.babel.js",
    "server": "nodemon ./server/server.js --exec babel-node --presets es2015,stage-0",
    "test": "mocha --compilers js:babel-register --require babel-polyfill --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 6.x",
    "npm": ">= 3.x"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "crypto": "0.0.3",
    "express": "^4.14.0",
    "immutability-helper": "^2.0.0",
    "jsonwebtoken": "^7.1.9",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.15.0",
    "mongoose": "^4.7.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "react": "^15.3.0",
    "react-cookie": "^1.0.3",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.2.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-form": "^6.2.0",
    "redux-thunk": "^2.1.0",
    "sanitize.css": "^4.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-import-resolver-webpack": "^0.5.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.2.0",
    "morgan": "^1.7.0",
    "offline-plugin": "^3.4.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "8.0.2",
    "postcss-loader": "^0.10.1",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.4.1",
    "react-addons-test-utils": "^15.4.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-dashboard": "^0.1.5",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-merge": "^0.14.1"
  }
